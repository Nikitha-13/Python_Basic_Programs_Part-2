    "# Program to check is a string is Palindrome\n",
    "def palindrome(input_str):\n",
    "    input_str == input_str[ : : -1]\n",
    "\n",
    "user_input = input(\"Enter a string : \")\n",
    "\n",
    "if palindrome(user_input):\n",
    "    print(\"It is a Palindrome\")\n",
    "    \n",
    "else:\n",
    "    print(\"It is not a Plaindrome\")"
   ]
  }
